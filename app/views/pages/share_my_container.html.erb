			<div class="row">
				<div class="col-sm-6">
					<div class="header-card">
						<div class="calculator-description">
							<h2>Offset your moving costs</h2>
							<p>Use our simple simulator to estimate the spare space you will have if you book a container. Get an idea of how much of your moving costs could be offset.</p>
							<div class="get-quotes-button">
			 					<%= link_to "Get free quotes", "#", class: "btn-get-quotes" %>
							</div> 
						</div>			
					</div>
				</div>
				<div class="col-sm-6">
					<div class="form-card">
						<div class="calculator-form">
							<%= simple_form_for @container, url: calculator_path do |f| %>
							<div class="row">
								<div class="col-sm-12">
									<p>Your container size</p>
								</div>
							</div>
							<div class="row">
								<div class="col-xs-12">
									<%= f.input :container_size, label: false, collection: Container::CONTAINERS, prompt: "My container size" %>
								</div>
							</div>
							<div class="row">
								<div class="col-sm-12">
									<p>Door to door cost for a container</p>
								</div>
							</div>
							<div class="row">
								<div class="col-sm-7">
									<%= f.input :price, as: :integer, label: false, input_html: { class: 'large-input' },  placeholder: "Best price you got so far" %>
								</div>
								<div class="col-sm-5">
									<%= f.input :currency, label: false, input_html: { class: 'special' },  collection: Container::CURRENCIES, prompt: params[:currency]%>
								</div>
							</div>
							<div class="row">
								<div class="col-sm-12">
									<p>Volume of your move</p>
								</div>
							</div>
							<div class="row">
								<div class="col-sm-7">
									<%= f.input :volume, as: :integer, label: false, input_html: { class: 'large-input' }, placeholder: "Volume of my move" %>
								</div>
								<div class="col-sm-5">
									<%= f.input :metrics, label: false,input_html: { class: 'special' }, collection: Container::METRIC, prompt: params[:metrics] %>
								</div>
							</div>
							<div class="row">
								<div class="chart-container">
									<div class="col">
										<div class="calculator-chart-card flex-items-centered">
											<div class="chart-box gaugeChar small-chart-top-margin">
												<div class="chart-value">
													12
												</div>
												<%= image_tag "small-chart.svg", height: 50 %>
											</div>
											<div class="chart-title">
												<p>M3 OF EMPTY SPACE</p>
											</div>
										</div>
									</div>
									<div class="col">
										<div class="calculator-chart-card">
											<div class="chart-box gaugeChar ">
												<div class="chart-value large-font">
													2,400
												</div>
												<%= image_tag "small-chart.svg", height: 50, class: "large-img" %>
											</div>
											<div class="chart-title">
												<p>TOTAL EARNINGS POTENTIAL (USD)</p>
											</div>
										</div>
									</div>
									<div class="col">
										<div class="calculator-chart-card">
											<div class="chart-box gaugeChar small-chart-top-margin">
												<div class="chart-value">
													20'
												</div>
												<%= image_tag "small-chart.svg", height: 50 %>
											</div>
											<div class="chart-title">
												<p>CONTAINER NEEDED</p>
											</div>
										</div>
									</div>
								</div>
							</div>
							<% end %>
						</div>
						
					</div>
				</div>
			</div>
		</div>
